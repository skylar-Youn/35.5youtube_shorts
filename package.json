{
  "name": "capcut-style-editor",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "concurrently -k -p '[{name}]' -c blue,magenta \"npm:dev:server\" \"npm:dev:web\"",
    "dev:server": "bash -lc 'python3 -m backend.app || python -m backend.app'",
    "dev:web": "npm --prefix editor run dev",
    "build:web": "npm --prefix editor run build",
    "start:web": "npm --prefix editor run start",
    "postinstall": "npm --prefix editor install || true",
    "test:e2e": "playwright test tests/e2e.aliexpress.spec.ts"
  },
  "devDependencies": {
    "concurrently": "8.2.2",
    "@playwright/test": "1.45.2"
  }
}
